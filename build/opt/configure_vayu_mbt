rm -rf CMake*

cmake                                                   \
    -D CMAKE_BUILD_TYPE=Release                         \
    -D CMAKE_C_COMPILER=gcc \
    -D CMAKE_CXX_COMPILER=g++ \
    -D CMAKE_Fortran_COMPILER=gfortran \
    -D CFLAGS="-fPIC" \
    -D CXXFLAGS="-fPIC" \
    -D USE_MPI=0 \
    -D CXX_STD=11 \
    -D QWT_URL=${HOME}/qwt-6.1.2.tar.bz2 \
    ../../src



